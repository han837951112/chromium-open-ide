{
    "manifest_version": 2,
    "name": "OME2",
    "description": "从chromium跳转到ide",
    "version": "1.0",
    "permissions": [
        "contextMenus",
        "http://127.0.0.1:8989/*",
        "https://chromium-review.googlesource.com/*",
        "https://codereview.chromium.org/*",
        "https://cs.chromium.org/*"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": [
            "js/jquery.js",
            "js/bg.js"
        ]
    }
}